name = "{{{ project.name }}}"
uuid = "{{{ project.uuid }}}"
authors = [{{#each project.authors~}}
    "{{{~this.firstname }}}{{#if this.lastname}} {{{ this.lastname }}}{{/if~}}
        {{#if this.email~}}
        <{{{this.email}}}>
        {{~/if}}",
    {{~/each}}]
version = "{{{ project.version }}}"

[deps]

[compat]
julia = "{{{ julia.compat }}}"
